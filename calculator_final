<script type="text/javascript">
$(document).ready(function(){
	
	function displayVals() {
      var no = $('[name="fno"]').val();
      var twono = no*2;
      $('[name="tno"]').html(twono);
    }
 
    $("form").change(displayVals);
    displayVals();

});
